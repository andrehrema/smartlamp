Checks: 'clang-diagnostic,clang-analyzer-*,cppcoreguidelines-*,-*,readability-identifier-naming'
WarningAsError: true
AnalyzeTemporaryDtors: false
CheckOptions:
  - key: readability-identifier-naming.VariableCase
    value: 'camelBack'
  - key: readability-identifier-naming.MemberPrefix
    value: 'm_'
  - key: readability-identifier-naming.StaticVariablePattern
    value: '^s_.*'
